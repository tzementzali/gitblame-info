{
  "name": "gitblame-info",
  "displayName": "Git Blame Informations",
  "description": "Affiche les informations git blame de la ligne courante à gauche dans la barre de statut.",
  "version": "1.2.1",
  "icon": "icon.jpg",
  "publisher": "Tewfik",
  "repository": {
    "type": "git",
    "url": "https://github.com/tzementzali/gitblame-info.git"
  },
  "homepage": "https://github.com/tzementzali/gitblame-info",
  "bugs": {
    "url": "https://github.com/tzementzali/gitblame-info/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "gitblame-info.showDetails",
        "title": "Afficher les détails du commit"
      }
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "vsce package",
    "package:prepublish": "vsce package --pre-release"
  },
  "keywords": [
    "Tewfik",
    "Alphaconcept"
  ],
  "author": "Tewfik.",
  "license": "ISC"
}
